{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "gender",
    "phone"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        10
      ],
      "minimum" : 1,
      "maximum" : 10000
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Lorenza"
      ],
      "minimum" : 3,
      "maximum" : 10
    },
    "gender": {
      "type": "string",
      "default": "",
      "title": "The gender Schema",
      "examples": [
        "Female"
      ]
    },
    "phone": {
      "type": "integer",
      "default": 0,
      "title": "The phone Schema",
      "examples": [
        3312820936
      ],
      "minimum" : 10,
      "maximum" : 12
    }
  },
  "examples": [{
    "id": 10,
    "name": "Lorenza",
    "gender": "Female",
    "phone": 3312820936
  }]
}